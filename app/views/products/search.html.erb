<table>
    <thead>
        <th> Product name </th>
        <th> Product description </th>
    </thead>
    <tbody>
        <% @products.each do |product| %>
        <tr>
            <td> <%= product.title %> </td>
            <td> <%= product.description %></td>        
        </tr>
        <% end %>
    </tbody>
</table>
